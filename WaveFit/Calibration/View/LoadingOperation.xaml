<Window x:Class="WaveFit2.Calibration.View.LoadingOperation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WaveFit2.Calibration.View"
        mc:Ignorable="d"
        xmlns:hc="https://handyorg.github.io/handycontrol"
        Title="LoadingOperation" Height="150" Width="150" Background="Transparent">
    <Grid>
        <Grid>

            <hc:TransitioningContentControl x:Name="LoadingOperationR">
                <hc:CircleProgressBar IsIndeterminate="True" Margin="0,0,0,0" Background="Transparent"/>
            </hc:TransitioningContentControl>

        </Grid>
    </Grid>
</Window>
