<UserControl x:Class="WaveFit2.Audiogram.View.PrintTemplateUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:WaveFit2.Audiogram.View"
             xmlns:hc="https://handyorg.github.io/handycontrol"
             xmlns:md="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             mc:Ignorable="d"
             Height="28.7cm"
             Width="20.1cm"
             Background="White">

    <Grid Margin="0">
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="auto" />
            <RowDefinition Height="auto" />
            <RowDefinition Height="auto" />
            <RowDefinition Height="auto" />
            <RowDefinition Height="2*" />
            <RowDefinition Height="auto" />
            <RowDefinition Height="1*" />
            <RowDefinition Height="auto" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="30" />
        </Grid.RowDefinitions>

        <Grid Grid.Row="0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="140" />
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="20" />
            </Grid.ColumnDefinitions>

            <Image Grid.Column="0"
                   x:Name="LogoBox"
                   Source="/Resources/Logo_Wavetech.png" />

            <Grid Grid.Column="1" Grid.ColumnSpan="2">
                <Grid.RowDefinitions>
                    <RowDefinition Height="10" />
                    <RowDefinition Height="auto" />
                    <RowDefinition Height="auto" />
                    <RowDefinition Height="auto" />
                    <RowDefinition Height="auto" />
                    <RowDefinition Height="auto" />
                    <RowDefinition Height="auto" />
                    <RowDefinition Height="auto" />
                </Grid.RowDefinitions>
                <TextBlock Grid.Row="1"
                           x:Name="NameTextBox"
                           Text="WAVETECH SOLUÇOES TECNOLOGICAS LIMITADA"
                           HorizontalAlignment="Right"
                           FontSize="12" />
                <TextBlock Grid.Row="2"
                           x:Name="AddressTextBox"
                           Text="Rod. José Carlos Daux, 600 - KM 01 Sala G1 .02, João Paulo"
                           HorizontalAlignment="Right"
                           FontSize="12" />
                <TextBlock Grid.Row="3"
                           x:Name="CEPTextBox"
                           Text="CEP: 88030-000"
                           HorizontalAlignment="Right"
                           FontSize="12" />
                <TextBlock Grid.Row="4"
                           x:Name="CityTextBox"
                           Text="Florianópolis - SC"
                           HorizontalAlignment="Right"
                           FontSize="12" />
                <TextBlock Grid.Row="5"
                           x:Name="CNPJTextBox"
                           Text="CNPJ - 155.658.69/0002-31"
                           HorizontalAlignment="Right"
                           FontSize="12" />
                <TextBlock Grid.Row="6"
                           x:Name="TelephoneTextBox"
                           Text="Fone: (48)3025-5858​"
                           HorizontalAlignment="Right"
                           FontSize="12" />
            </Grid>
        </Grid>

        <Grid Grid.Row="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Border Grid.Row="0"
                    BorderBrush="Black"
                    BorderThickness="1"
                    Background="LightGray">
                <TextBlock Grid.Row="1"
                       Text="Avaliação Audiológica"
                       FontWeight="Bold"
                       HorizontalAlignment="Center"
                       FontSize="12" />
            </Border>
        </Grid>

        <Border Grid.Row="2"
                BorderBrush="Black"
                BorderThickness="1">

            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>

                <Grid Grid.Row="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>

                    <Border Grid.Column="0"
                                BorderBrush="Black"
                                BorderThickness="1">
                        <Grid Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="5" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="5" />
                            </Grid.RowDefinitions>
                            <TextBlock Grid.Row="1"
                                       Text="Funcionário/Paciente:"
                                       FontWeight="Bold"
                                       Margin="10,0,0,0" />
                            <TextBlock Grid.Row="2"
                                       x:Name="PatientName"
                                       Margin="10,0,0,0" />
                        </Grid>
                    </Border>

                    <Border Grid.Column="1"
                                BorderBrush="Black"
                                BorderThickness="1">
                        <Grid Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="5" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="5" />
                            </Grid.RowDefinitions>
                            <TextBlock Grid.Row="1"
                                       Text="Idade:"
                                       FontWeight="Bold"
                                       Margin="10,0,0,0" />
                            <TextBlock Grid.Row="2"
                                       x:Name="PatientAge"
                                       Margin="10,0,0,0" />
                        </Grid>
                    </Border>

                    <Border Grid.Column="2"
                                BorderBrush="Black"
                                BorderThickness="1">
                        <Grid Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="5" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="5" />
                            </Grid.RowDefinitions>
                            <TextBlock Grid.Row="1"
                                       Text="Sexo:"
                                       FontWeight="Bold"
                                       Margin="10,0,0,0" />
                            <TextBlock Grid.Row="2"
                                       x:Name="PatientGender"
                                       Margin="10,0,0,0" />
                        </Grid>
                    </Border>

                    <Border Grid.Column="3"
                                BorderBrush="Black"
                                BorderThickness="1">
                        <Grid Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="5" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="5" />
                            </Grid.RowDefinitions>
                            <TextBlock Grid.Row="1"
                                       Text="Documento:"
                                       FontWeight="Bold"
                                       Margin="10,0,0,0" />
                            <TextBlock Grid.Row="2"
                                       x:Name="PatientID"
                                       Margin="10,0,0,0" />
                        </Grid>
                    </Border>
                </Grid>

                <Grid Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>

                    <Border Grid.Column="0"
                                BorderBrush="Black"
                                BorderThickness="1">
                        <Grid Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="5" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="5" />
                            </Grid.RowDefinitions>
                            <TextBlock Grid.Row="1"
                                       Text="Audiômetro:"
                                       FontWeight="Bold"
                                       Margin="10,0,0,0" />

                            <TextBlock Grid.Row="2"
                                       x:Name="AudiometerTextBox"
                                       Margin="10,0,0,0" ><Run Text="AMPLIVOX 270" /><Run Language="pt-br" Text=" (Última Aferição: 04/08/2023)" /></TextBlock> <!-- sp: (Última Aferição: 04/08/2023)"-->
                        </Grid>
                    </Border>

                    <Border Grid.Column="1"
                                BorderBrush="Black"
                                BorderThickness="1">
                        <Grid Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="5" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="5" />
                            </Grid.RowDefinitions>
                            <TextBlock Grid.Row="1"
                                       Text="Data do Exame:"
                                       FontWeight="Bold"
                                       Margin="10,0,0,0" />
                            <TextBlock Grid.Row="2"
                                       x:Name="ExamDate"
                                       Text=""
                                       Margin="10,0,0,0" />
                        </Grid>
                    </Border>
                </Grid>
            </Grid>
        </Border>

        <Grid Grid.Row="3">
            <Grid.RowDefinitions>
                <RowDefinition Height="10" />
                <RowDefinition Height="auto" />
                <RowDefinition Height="*" />
                <RowDefinition Height="20" />
            </Grid.RowDefinitions>

            <TextBlock Grid.Row="1"
                       Text="MEATOSCOPIA (Encaminhar o Funcionário/Paciente ao Otorrinolaringologista)"
                       FontSize="12"
                       HorizontalAlignment="Center" />

            <Grid Grid.Row="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="20" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="20" />
                </Grid.ColumnDefinitions>

                <Grid Grid.Column="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="auto" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>

                    <Border BorderBrush="black"
                            BorderThickness="1"
                            Grid.Row="0"
                            Grid.Column="0"
                            Background="#ffcdd2">
                        <TextBlock Text="Orelha Direita"
                                   Margin="10,0,40,0"
                                   FontSize="12" />
                    </Border>

                    <Border BorderBrush="black"
                            BorderThickness="1"
                            Grid.Row="1"
                            Grid.Column="0"
                            Background="#bbdefb">
                        <TextBlock Text="Orelha Esquerda"
                                   Margin="10,0,40,0"
                                   FontSize="12" />
                    </Border>

                    <Border BorderBrush="black"
                            BorderThickness="1"
                            Grid.Row="0"
                            Grid.Column="1">
                        <TextBlock x:Name="ODMeatoscopy"
                                   Text=""
                                   Foreground="Red"
                                   Margin="10,0,0,0"
                                   FontSize="12" />
                    </Border>

                    <Border BorderBrush="black"
                            BorderThickness="1"
                            Grid.Row="1"
                            Grid.Column="1">
                        <TextBlock x:Name="OEMeatoscopy"
                            Text=""
                                   Foreground="Blue"
                                   Margin="10,0,0,0"
                                   FontSize="12" />
                    </Border>
                </Grid>
            </Grid>
        </Grid>

        <Grid Grid.Row="4">
