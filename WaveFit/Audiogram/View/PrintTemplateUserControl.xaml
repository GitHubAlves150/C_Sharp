                    <Border Grid.Column="4"
                            BorderBrush="Black"
                            BorderThickness="1">
                        <TextBlock x:Name="ODDi"
                                   Text=""
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="12"
                                   Foreground="Red" />
                    </Border>

                    <Border Grid.Column="5"
                            BorderBrush="Black"
                            BorderThickness="1">
                        <TextBlock x:Name="ODTri"
                                   Text=""
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="12"
                                   Foreground="Red" />
