                        <TextBlock x:Name="ODIntensid"
                                   Text=""
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="12"
                                   Foreground="Red" />
                    </Border>

                    <Border Grid.Column="3"
                            BorderBrush="Black"
                            BorderThickness="1">
                        <TextBlock x:Name="ODMono"
                                   Text=""
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="12"
                                   Foreground="Red" />
