                    <Border Grid.Column="5"
                            BorderBrush="Black"
                            BorderThickness="1">
                        <TextBlock x:Name="OETri"
                                   Text=""
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="12"
                                   Foreground="Blue" />
                    </Border>
                </Grid>
            </Grid>

            <Grid Grid.Column="2">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="1.5*" />
                    <RowDefinition Height="1.25*" />
                    <RowDefinition Height="1.25*" />
                </Grid.RowDefinitions>

                <Grid Grid.Row="0">
                    <TextBlock Text="Mascaramento (dB)"
