                            Background="LightGray">
                        <TextBlock Text="Intensidade"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="12" />
                    </Border>

                    <Border Grid.Column="3"
                            BorderBrush="Black"
                            BorderThickness="1"
                            Background="LightGray">
                        <TextBlock Text="Monossil"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="12" />
                    </Border>

                    <Border Grid.Column="4"
                            BorderBrush="Black"
                            BorderThickness="1"
                            Background="LightGray">
                        <TextBlock Text="Dissil"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="12" />
                    </Border>

                    <Border Grid.Column="5"
                            BorderBrush="Black"
                            BorderThickness="1"
                            Background="LightGray">
                        <TextBlock Text="Trissil"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="12" />
                    </Border>
                </Grid>

                <Grid Grid.Row="2">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="20" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="20" />
                    </Grid.ColumnDefinitions>

                    <Border Grid.Column="1"
                            BorderBrush="Black"
                            BorderThickness="1"
                            Background="#eceff1">
                        <TextBlock Text="Pal. Faladas"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="12" />
                    </Border>

                    <Border Grid.Column="2"
                            BorderBrush="Black"
                            BorderThickness="1">

                        <TextBlock Text="X"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="12" />
                    </Border>

                    <Border Grid.Column="3"
                            BorderBrush="Black"
                            BorderThickness="1">
                        <TextBlock x:Name="NWordsMono"
