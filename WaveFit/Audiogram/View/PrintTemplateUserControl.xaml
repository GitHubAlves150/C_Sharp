                    <Border Grid.Column="4"
                            BorderBrush="Black"
                            BorderThickness="1">
                        <TextBlock x:Name="OEDi"
                                   Text=""
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="12"
                                   Foreground="Blue" />
