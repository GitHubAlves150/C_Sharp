                                   FontSize="12" />
                    </Border>

                    <Border Grid.Column="2"
                            BorderBrush="Black"
                            BorderThickness="1">
                        <TextBlock x:Name="OEIntensid"
                                   Text=""
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="12"
                                   Foreground="Blue" />
                    </Border>

                    <Border Grid.Column="3"
                            BorderBrush="Black"
                            BorderThickness="1">
                        <TextBlock x:Name="OEMono"
                                   Text=""
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="12"
                                   Foreground="Blue" />
