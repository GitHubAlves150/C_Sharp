<UserControl x:Class="WaveFit2.Audiogram.View.StaticAudiographUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:oxy="clr-namespace:OxyPlot.Wpf;assembly=OxyPlot.Wpf"
             xmlns:local="clr-namespace:WaveFit2.Audiogram.View"
             mc:Ignorable="d"
             d:DesignHeight="200" d:DesignWidth="200" Background="Transparent">
        <Grid>
            <oxy:PlotView x:Name="staticPlot" Model="{Binding AudiogramPlot}" />
        </Grid>
</UserControl>